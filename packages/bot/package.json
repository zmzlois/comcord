{
  "name": "@comcord/bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:w": "tsc -w",
    "start": "pnpm run with-env node .",
    "start:w": "nodemon -w build -w ../../.env.local -e js,ts --exec 'pnpm run start'",
    "bot:dev": "concurrently --handle-input --default-input-target 1 \"pnpm run build:w\" \"pnpm run start:w\"",
    "build:changelog": "npx @discordx/changelog --src src",
    "vic:dev": "node-loader ts-node/esm/transpile-only src/main.ts",
    "watch": "nodemon --watch '*/**'--exec 'pnpm run dev'",
    "with-env": "dotenv -e ../../.env.local"
  },
  "dependencies": {
    "tsconfig": "workspace:*",
    "@comcord/utils": "workspace:*",
    "@discordx/importer": "^1.3.0",
    "@discordx/koa": "^1.2.1",
    "@discordx/pagination": "^3.5.0",
    "@discordx/utilities": "^6.2.0",
    "@huggingface/inference": "^2.6.6",
    "@koa/router": "^12.0.1",
    "@t3-oss/env-core": "^0.9.2",
    "buffer-image-size": "^0.6.4",
    "concurrently": "^8.2.2",
    "discord.js": "^14.14.1",
    "discordx": "^11.9.0",
    "dotenv": "^16.4.5",
    "gif-encoder": "^0.7.2",
    "koa": "^2.14.2",
    "openai": "^4.29.2",
    "png-js": "^1.0.0",
    "puppeteer": "^22.6.0",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.1",
    "waifuvault-node-api": "^1.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/gif-encoder": "^0.7.4",
    "@types/koa": "^2.13.12",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "5.3.3"
  }
}